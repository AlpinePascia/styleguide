<!-- scripts. -->
<script src="/{{this.options.custom}}/requirejs-config.js"></script>

<script type="text/javascript">
    require.config({
        baseUrl: '/{{this.options.custom}}/'
    });

    require([
        'https://cdn.jsdelivr.net/npm/clipboard@2/dist/clipboard.min.js',
        '/styleguide/kss-assets/vendor/prism.js',
        '/styleguide/kss-assets/js/kss.js',
    ], function(ClipboardJS) {
        new ClipboardJS('.clipboard-btn');
        window.Prism.highlightAll();
    });
</script>
<div>
    {{#each this.options}}
        Key: {{@key}} Value = {{this}}
    {{/each}}
</div>

<div>
    {{#each this.styleGuide.data}}
        Key: {{@key}} Value = {{this}}
    {{/each}}
</div>


