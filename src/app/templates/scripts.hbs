<script type="text/javascript">
    var dest = '{{this.options.destination}}';
    var isStaging = !/workplace/.test(dest);
    // isStaging = true;
    var requireConfig = document.createElement('script');
    requireConfig.src = isStaging
        ? '/{{this.options.custom}}/requirejs-config.min.js'
        : '/{{this.options.custom}}/requirejs-config.js';


    document.getElementsByTagName('head')[0].appendChild(requireConfig);
    if (isStaging) {
        var minResolver = document.createElement('script');
        minResolver.src = '/{{this.options.custom}}/requirejs-min-resolver.min.js';
        document.getElementsByTagName('head')[0].appendChild(minResolver);
    }
</script>

<!-- <script src="/{{this.options.custom}}/requirejs-config.js"></script>
<script src="/{{this.options.custom}}/requirejs-min-resolver.min.js"></script> -->


<script type="text/javascript">
    require.config({
        baseUrl: '/{{this.options.custom}}/'
    });

    require([
        'https://cdn.jsdelivr.net/npm/clipboard@2/dist/clipboard.min.js',
        '/styleguide/kss-assets/vendor/prism.js',
        '/styleguide/kss-assets/js/kss.js',
    ], function(ClipboardJS) {
        new ClipboardJS('.clipboard-btn');
        window.Prism.highlightAll();
    });
</script>

